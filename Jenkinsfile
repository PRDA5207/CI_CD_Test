pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Goodbye') {
            steps {
                echo 'good bye'
            }
        }
        stage('sh') {
            steps {
                
                sh 'echo \'from shell\''
            }
        }
        
    }
        
}
